{
	"name": "authy-decryptor",
	"version": "1.0.0",
	"description": "CLI tool to decrypt Authy encrypted authenticator backup tokens",
	"type": "module",
	"bin": {
		"authy-decryptor": "./dist/bin/cli.js"
	},
	"scripts": {
		"build": "tsc",
		"start": "node dist/bin/cli.js",
		"dev": "tsx watch bin/cli.ts -i ./encyrpted.csv -o decrypted.json -u tokens.plain"
	},
	"dependencies": {
		"commander": "^11.0.0",
		"csv-parse": "^6.1.0"
	},
	"devDependencies": {
		"@types/node": "^20.10.5",
		"tsx": "^4.20.6",
		"typescript": "^5.3.2"
	}
}
