{
	"name": "authy-decryptor",
	"version": "1.0.0",
	"description": "CLI tool to decrypt Authy encrypted authenticator backup tokens",
	"type": "module",
	"main": "./dist/bin/cli.js",
	"bin": {
		"authy-decryptor": "./dist/bin/cli.js"
	},
	"files": [
		"dist"
	],
	"release-it": {
		"git": {
			"requireCleanWorkingDir": true
		},
		"plugins": {
			"@release-it/conventional-changelog": {
				"preset": "conventionalcommits"
			}
		},
		"github": {
			"release": true,
			"assets": [
				"build/*"
			]
		}
	},
	"scripts": {
		"build": "ncc build ./bin/cli.ts -o dist/bundle && node nexe-build.js",
		"test": "jest",
		"release": "npm run build && release-it",
		"release:alpha": "npm run build && release-it prerelease --preRelease=alpha",
		"release:beta": "npm run build && release-it prerelease --preRelease=beta",
		"release:rc": "npm run build && release-it prerelease --preRelease=rc"
	},
	"keywords": [],
	"author": "nick22985",
	"license": "MIT",
	"dependencies": {
		"commander": "^11.0.0",
		"csv-parse": "^6.1.0",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@release-it/conventional-changelog": "^10.0.1",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.20",
		"@vercel/ncc": "^0.38.4",
		"dotenv": "^17.2.3",
		"jest": "^29.7.0",
		"nexe": "^5.0.0-beta.4",
		"release-it": "^19.0.5",
		"ts-jest": "^29.1.2",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3"
	}
}
