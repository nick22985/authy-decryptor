{
	"name": "@nick22985/authy-decryptor",
	"version": "0.1.1",
	"publishConfig": {
		"access": "public"
	},
	"description": "CLI tool to decrypt Authy encrypted authenticator backup tokens",
	"type": "module",
	"main": "./dist/bundle/index.js",
	"bin": {
		"authy-decryptor": "./dist/bundle/index.js"
	},
	"files": [
		"dist"
	],
	"release-it": {
		"git": {
			"requireCleanWorkingDir": true
		},
		"plugins": {
			"@release-it/conventional-changelog": {
				"preset": "conventionalcommits"
			}
		},
		"github": {
			"release": true,
			"assets": [
				"build/*"
			]
		}
	},
	"scripts": {
		"build": "rm -rf ./build && ncc build ./bin/cli.ts -o dist/bundle && node nexe-build.js",
		"test": "jest",
		"release": "npm run build && release-it",
		"release:alpha": "npm run build && release-it prerelease --preRelease=alpha",
		"release:beta": "npm run build && release-it prerelease --preRelease=beta",
		"release:rc": "npm run build && release-it prerelease --preRelease=rc"
	},
	"keywords": [],
	"author": "nick22985",
	"license": "MIT",
	"dependencies": {
		"commander": "^14.0.1",
		"csv-parse": "^6.1.0",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@release-it/conventional-changelog": "^10.0.1",
		"@types/jest": "^30.0.0",
		"@types/node": "^24.7.0",
		"@vercel/ncc": "^0.38.4",
		"dotenv": "^17.2.3",
		"jest": "^30.2.0",
		"nexe": "^5.0.0-beta.4",
		"release-it": "^19.0.5",
		"string-width": "^8.1.0",
		"ts-jest": "^29.4.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.9.3"
	},
	"directories": {
		"test": "tests"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nick22985/authy-decryptor.git"
	},
	"bugs": {
		"url": "https://github.com/nick22985/authy-decryptor/issues"
	},
	"homepage": "https://github.com/nick22985/authy-decryptor#readme"
}
